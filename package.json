		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-simple-import-sort": "^4.0.0",
		"jest": "^24.9.0",
		"prettier": "^1.19.1",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.5.2",
		"typescript": "^3.7.2"
